    {
      "id": "signal_station_*[symbol]",
      "type": "symbol",
      "source": "seamarks",
      "source-layer": "seamarks",
      "filter": [
        "any",
        ["==", "seamark:type", "signal_station_traffic"],
        ["==", "seamark:type", "signal_station_warning"]
      ],
      "layout": {
        "icon-allow-overlap": true,
        "icon-ignore-placement": true,
        "text-optional": true,
        "text-allow-overlap": false,
        "text-field": [
          "format",
          [
            "concat",
            "SS(",
            [
              "match",
              ["get", "seamark:signal_station_traffic:category"],
              "bridge_passage",
              "Bridge",
              "port_entry_departure",
              "Port",
              [
                "coalesce",
                ["get", "seamark:signal_station_traffic:category"],
                ["get", "seamark:signal_station_warning:category"]
              ]
            ],
            ")"
          ],
          {},
          "\n",
          {},
          [
            "coalesce",
            ["get", "seamark:name"],
            ["get", "ref"],
            ["get", "name"],
            ["get", "operator"]
          ],
          {},
          "\n",
          {},
          [
            "case",
            ["has", "seamark:signal_station_traffic:channel"],
            [
              "concat",
              "Ch.",
              ["get", "seamark:signal_station_traffic:channel"]
            ],
            ["has", "seamark:signal_station_warning:channel"],
            [
              "concat",
              "Ch.",
              ["get", "seamark:signal_station_warning:channel"]
            ],
            ""
          ],
          {}
        ],
        "text-size": 10,
        "text-justify": "left",
        "text-anchor": "left",
        "icon-image": "signal_station",
        "icon-size": 0.4,
        "text-radial-offset": 1
      },
      "minzoom": 11
    },
    {
      "id": "small_craft_facility[symbol]",
      "type": "symbol",
      "source": "seamarks",
      "source-layer": "seamarks",
      "filter": ["any", ["==", "seamark:type", "small_craft_facility"]],
      "layout": {
        "icon-allow-overlap": true,
        "icon-ignore-placement": true,
        "text-optional": true,
        "text-allow-overlap": false,
        "icon-image": [
          "match",
          ["get", "seamark:small_craft_facility:category"],
          "boat_hoist",
          "small_craft_facility/boat_hoist",
          "boatyard",
          "small_craft_facility/boatyard",
          "fuel_station",
          "small_craft_facility/fuel_station",
          "pump-out",
          "small_craft_facility/pump_out",
          "slipway",
          "small_craft_facility/slipway",
          "visitor_berth",
          "small_craft_facility/visitor_berth",
          "visitor_mooring",
          "small_craft_facility/visitor_mooring",
          ""
        ],
        "icon-size": 0.3,
        "icon-keep-upright": false,
        "text-field": [
          "format",
          [
            "coalesce",
            ["get", "seamark:name"],
            ["get", "ref"],
            ["get", "name"],
            ["get", "operator"]
          ],
          {},
          "\n",
          {},
          [
            "case",
            [
              "any",
              ["has", "maxstay"],
              ["has", "maxdraft"],
              ["has", "maxlength"],
              ["has", "maxwidth"],
              ["has", "maxweight"]
            ],
            [
              "concat",
              "max",
              [
                "case",
                ["has", "maxstay"],
                ["concat", ". ", ["get", "maxstay"]],
                ""
              ],
              [
                "case",
                ["has", "maxweight"],
                ["concat", ". ", ["get", "maxweight"], "t"],
                ""
              ],
              [
                "case",
                ["has", "maxdraft"],
                ["concat", ". ", ["get", "maxdraft"], "m draft"],
                ""
              ],
              [
                "case",
                ["has", "maxlength"],
                ["concat", ". ", ["get", "maxlength"], "m LOA"],
                ""
              ],
              [
                "case",
                ["has", "maxwidth"],
                ["concat", ". ", ["get", "maxwidth"], "m wide"],
                ""
              ]
            ],
            ""
          ],
          {}
        ],
        "text-size": 10,
        "text-max-width": 20,
        "text-justify": "left",
        "text-anchor": "left",
        "text-radial-offset": 0.9
      },
      "minzoom": 11
    },
